{
  "name": "AmazonDoge",
  "version": "1.0",
  "description": "A Doge silently judges you every time you visit Amazon.com.",
   "content_scripts": [{
    "css": ["style.css"],
    "js": ["content.js"],
    "matches": ["https://*.amazon.com/*"]
  }],
  "manifest_version": 2,
  "web_accessible_resources": ["images/doge.png"],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}