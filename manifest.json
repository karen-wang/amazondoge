{
  "name": "AmazonDoge",
  "version": "1.1",
  "description": "let a doge silently judge you every time you visit Amazon",
   "content_scripts": [{
    "css": ["style.css"],
    "js": ["content.js"],
    "matches": [
    	"*://*.amazon.com/*",
    	"*://*.amazon.ae/*",
		"*://*.amazon.at/*",
		"*://*.amazon.ca/*",
		"*://*.amazon.cn/*",
		"*://*.amazon.co.jp/*",
		"*://*.amazon.co.uk/*",
		"*://*.amazon.com.au/*",
		"*://*.amazon.com.br/*",
		"*://*.amazon.com.mx/*",
		"*://*.amazon.com.sg/*",
		"*://*.amazon.com.tr/*",
		"*://*.amazon.com/*",
		"*://*.amazon.de/*",
		"*://*.amazon.es/*",
		"*://*.amazon.fr/*",
		"*://*.amazon.ie/*",
		"*://*.amazon.in/*",
		"*://*.amazon.it/*",
		"*://*.amazon.nl/*"]
  }],
  "manifest_version": 3,
  "web_accessible_resources": [
  	{
      "resources": [
        "images/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}